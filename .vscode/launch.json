    "version": "0.2.0",
    "configurations": [
        {
            "name": "Analisys",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "30"
            }
        },
        {
            "name": "run",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "justMyCode": true,
            "module": "app",
            "args": [
                "run",
                "--path",
                "input/test.csv"
            ],
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "30"
            }
        },
        {
            "name": "run-async",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "justMyCode": true,
            "module": "app",
            "args": [
                "run-async",
                "--path",
                "input/test.csv"
            ],
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "30"
            }
        }
    ]
}